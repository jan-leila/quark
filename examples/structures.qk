
enum status {
    pending;
    error;
    success;
}

struct <T> contact {
    string name;
    int age;
    string email?;
}

function <T> add_numbers(a, b){
    return a + b;
}

event <T> read_effect in {
    string file_name;
}
out {
    string data;
}

monad <T> optional {
    boolean set;
    T value;
}
bind (callback) {
    if(set){
        return callback(value);
    }
    return null;
}
reduce (default_value) {
    if(set){
        return value;
    }
    return default_value;
}
